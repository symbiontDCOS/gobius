[Distribution]
Distribution=fedora
Release=32

[Output]
Bootable=yes
Format=gpt_ext4
Hostname=installer
Output=installer_2107rc1.img.xz
XZ=yes

[Packages]
Cache=../__sharedCache__
Packages=
	btrfs-progs
	dialog
	dmidecode
	htop
	openssh-server
	procps-ng
	usbutils
	zstd
	bmap-tools
	python3-dialog
	python3-pytz
	python3-requests
	dnf
	vim-enhanced

[Partitions]
RootSize=2G

[Validation]
BMap=yes
CheckSum=yes
Autologin=yes


